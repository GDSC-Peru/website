{
  "name": "gdsc-website",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:css": "stylelint **/*.{css,scss,sass}",
    "e2e": "ng e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "npx cypress open",
    "serve:staging": "ng serve --configuration=staging",
    "serve:prod": "ng serve --configuration=production",
    "build:staging": "ng build --prod --configuration=staging",
    "build:prod": "ng build --prod --configuration=production",
    "generate:stats": "ng build --prod --stats-json",
    "start:analyzer": "webpack-bundle-analyzerwebsite/stats.json",
    "analyzer": "npm run generate:stats && npm run start:analyzer",
    "dev:ssr": "ng run website:serve-ssr",
    "serve:ssr": "node dist/website/server/main.js",
    "build:ssr": "ng build && ng run website:server",
    "prerender": "ng run website:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "^0.1303.3",
    "@angular-devkit/schematics": "^13.3.3",
    "@angular/animations": "^13.3.3",
    "@angular/common": "~13.3.3",
    "@angular/compiler": "~13.3.3",
    "@angular/core": "~13.3.3",
    "@angular/fire": "^7.3.0",
    "@angular/forms": "~13.3.3",
    "@angular/platform-browser": "~13.3.3",
    "@angular/platform-browser-dynamic": "~13.3.3",
    "@angular/platform-server": "~13.3.3",
    "@angular/router": "~13.3.3",
    "@nguniversal/express-engine": "^13.1.0",
    "@sentry/angular": "^6.19.6",
    "@sentry/tracing": "^6.19.6",
    "express": "^4.15.2",
    "npm-check-updates": "^12.5.9",
    "rxjs": "~7.5.5",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.3",
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.3.3",
    "@angular/compiler-cli": "~13.3.3",
    "@nguniversal/builders": "^13.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "codelyzer": "^6.0.2",
    "cypress": "^9.5.4",
    "eslint": "^8.13.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.2.4",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-react": "latest",
    "husky": "^7.0.4",
    "jasmine-core": "~4.1.0",
    "jasmine-spec-reporter": "~7.0.0",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "protractor": "~7.0.0",
    "stylelint": "^14.7.1",
    "stylelint-config-standard": "^25.0.0",
    "ts-node": "~10.7.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.3",
    "webpack-bundle-analyzer": "^4.5.0",
    "firebase-admin": "^9.11.1",
    "firebase-functions": "^3.6.0"
  },
  "lint-staged": {
    "*.css": [
      "npx stylelint **/*.scss",
      "stylelint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint:css && ng lint && ng test"
    }
  }
}